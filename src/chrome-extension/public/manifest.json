{
  "name": "Chrome Alive Extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["/content.js"],
      "run_at": "document_start"
    }
  ],
  "host_permissions": ["https://*/*", "http://*/*", "*://*/*"],
  "devtools_page": "/devtools-entrypoint.html",
  "key": ""
}
