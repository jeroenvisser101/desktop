{
  "name": "desktop",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "NODE_OPTIONS=\"-r ts-node/register --no-warnings\" node scripts/cli.ts preview",
    "dev": "NODE_OPTIONS=\"-r ts-node/register --no-warnings\" node scripts/cli.ts dev",
    "build:run": "NODE_OPTIONS=\"-r ts-node/register --no-warnings\" node scripts/cli.ts build",
    "build:import": "NODE_OPTIONS=\"-r ts-node/register --no-warnings\" node scripts/copySources.ts && npm install",
    "build": "yarn typecheck && yarn build:run",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "yarn build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --dir",
    "build:win": "yarn build && electron-builder --win",
    "build:mac": "yarn build && electron-builder --mac",
    "build:linux": "yarn build && electron-builder --linux",
    "unpack:asar": "npx @electron/asar extract dist/mac-arm64/Ulixee.app/Contents/Resources/app.asar dist/mac-arm64/app_unpacked"
  },
  "dependencies": {
    "@argonprotocol/localchain": "0.0.4",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@ulixee/cloud": "2.0.0-alpha.29",
    "@ulixee/commons": "2.0.0-alpha.29",
    "@ulixee/datastore": "2.0.0-alpha.29",
    "@ulixee/datastore-core": "2.0.0-alpha.29",
    "@ulixee/datastore-plugins-hero": "2.0.0-alpha.29",
    "@ulixee/default-browser-emulator": "2.0.0-alpha.29",
    "@ulixee/desktop-core": "2.0.0-alpha.29",
    "@ulixee/desktop-interfaces": "2.0.0-alpha.29",
    "@ulixee/hero-core": "2.0.0-alpha.29",
    "@ulixee/hero-interfaces": "2.0.0-alpha.29",
    "@ulixee/js-path": "2.0.0-alpha.29",
    "@ulixee/net": "2.0.0-alpha.29",
    "@ulixee/platform-specification": "2.0.0-alpha.29",
    "@ulixee/platform-utils": "2.0.0-alpha.29",
    "@ulixee/unblocked-specification": "2.0.0-alpha.29",
    "better-sqlite3": "^11.1.2",
    "electron-log": "^5.1.7",
    "electron-updater": "^6.3.4",
    "moment": "^2.29.4",
    "nanoid": "^3.3.6",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "electron-clipboard-ex": "^1.3.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/asar": "^3.2.13",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tailwindcss/forms": "^0.5.7",
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.14.8",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@webcomponents/custom-elements": "^1.6.0",
    "autoprefixer": "^10.4.17",
    "cac": "^6.7.14",
    "electron": "^31.0.2",
    "electron-builder": "^25.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "magic-string": "^0.30.11",
    "moment": "^2.29.4",
    "picocolors": "^1.1.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prismjs": "^1.29.0",
    "qrcode": "^1.5.3",
    "rollup": "^4.21.3",
    "sass-embedded": "^1.78.0",
    "swc": "^1.0.11",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-native": "^2.2.1",
    "vite-plugin-prismjs": "^0.0.11",
    "vite-plugin-static-copy": "^0.17",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.30",
    "vue-json-pretty": "^2.3.0",
    "vue-router": "^4.4.4",
    "vue-tsc": "^2.0.22"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependenciesMeta": {
    "@argonprotocol/localchain": {}
  },
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "composite": true
    },
    "swc": true
  },
  "packageManager": "yarn@4.4.1"
}
